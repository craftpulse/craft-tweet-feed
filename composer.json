{
    "name": "craftpulse/craft-tweet-feed",
    "description": "Get the latest tweets from a Twitter handle with your own design",
    "type": "craft-plugin",
    "version": "4.2.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "tweets",
        "twitter"
    ],
    "support": {
        "email": "support@percipio.london",
        "docs": "https://github.com/percipioglobal/craft-tweet-feed/blob/v4/README.md",
        "issues": "https://github.com/percipioglobal/craft-tweet-feed/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Percipio.London",
            "homepage": "https://percipio.london"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0.2",
        "craftcms/cms": "^4.0.0-beta.1",
        "guzzlehttp/oauth-subscriber": "^0.6.0"
    },
    "require-dev": {
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "percipiolondon\\tweetfeed\\": "src/"
        }
    },
    "extra": {
        "name": "Tweet Feed",
        "handle": "tweetfeed",
        "developer": "percipiolondon",
        "developerUrl": "https://percipio.london",
        "documentationUrl": "https://github.com/percipioglobal/craft-tweet-feed/blob/v4/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/percipioglobal/craft-tweet-feed/v4/CHANGELOG.md",
        "class": "percipiolondon\\tweetfeed\\TweetFeed"
    },
    "scripts": {
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
